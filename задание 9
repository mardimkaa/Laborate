// задание 1

#include <stdio.h>

int main()
{
   int t, r;
   printf("Введите количество секунд:");
   scanf("%d", &t);
   r = t % 60;
   printf("С последней минуты прошло %d секунд", r);
   return 0;
}

//задание 2
#include <stdio.h>

int main()
{
   int K, N;
   printf("Введите день:");
   scanf("%d", &K);
   N = K % 7;
   printf("%d", N);
   return 0;
}
