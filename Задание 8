#include <stdio.h>
#include <math.h>
int main()
{
    int N;
    printf("Введите номер задания");
    scanf("%d", &N);
    if (N == 1) {
        int A;
        printf("Введите размер файла в байтах:");
        scanf("%d", &A);
        if ((A > 1024) && (A % 1024 != 0)) {
            A = A / 1024 + 1;
        }
        else if ((A > 1024) && (A % 1024 == 0)) {
            A = A / 1024;

        }
        else {
            A = 1;
        }
        printf("Данный фаил занимает %d килобайт", A);
        return 0;
    }

    if (N == 2) {
        int A, B, C;
        printf("Введите длинну отрезка A:");
        scanf("%d", &A);
        printf("Введите длинну отрезка B:");
        scanf("%d", &B);
        if (A < B)
        {
            printf("Введите другие числа");
            return 0;
        }
        else
        {
            C = A / B;
            printf("%d отрезков В поместяться в А", C);
            return 0;
        }

    }

    if (N == 3) {
        int A, B, C;
        printf("Введите длинну отрезка A:");
        scanf("%d", &A);
        printf("Введите длинну отрезка B:");
        scanf("%d", &B);
        if (A < B)
        {
            printf("Введите другие числа");
            return 0;
        }
        else
        {
            C = A / B;
            C = C * B;
            C = A - C;
            printf("Размер не занятой части А равен %d", C);
            return 0;
        }

    }

    if (N == 4) {
        int A, B, C;
        printf("Введите число:");
        scanf("%d", &A);
        if ((A > 9) && (A < 100)) {
            B = A / 10;
            C = A % 10;
            printf("%d", C * 10 + B);
        }
        else {
            printf("Число не двухзнач!!");
        }
    }

    if (N == 5) {
        int A, B, C;
        printf("Введите число:");
        scanf("%d", &A);
        if ((99 < A) && (A < 1000)) {
            B = A / 100;
            C = A - B * 100;
            A = C * 10 + B;
            printf("%d", A);
            return 0;
        }
        else {
            printf("Число не трехзначное");
            return 0;
        }
    }

}
